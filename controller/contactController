const express=require("express")
const phonemodels=require("../model/contactmodel")
const router=express.Router()

router.post("/add",async(req,res)=>
{
    let data=req.body
    let model=new phonemodels(data)
    let result=await model.save()
    res.json(
        {
            status:"success"
        }
    )
})

router.post("/search",async(req,res)=>
{
    let data=req.body
    let result=await phonemodels.find(data)
    res.json(result)
})

module.exports=router